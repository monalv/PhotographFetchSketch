<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.getimages"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name="com.example.getimages.FriendPickerApplication"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.getimages.FriendPickerSampleActivity"
            android:label="@string/app_name"
          >
                      <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        
        </activity>
        <activity
            android:name="com.example.getimages.PickFriendsActivity"
            android:label="Pick Friends" >
        </activity>
        <activity
            android:name="com.facebook.LoginActivity"
            android:label="@string/app_name"
            android:theme="@android:style/Theme.Translucent.NoTitleBar" >
        </activity>

        <meta-data
            android:name="com.facebook.sdk.ApplicationId"
            android:value="@string/app_id" />

        <activity
            android:name="com.example.getimages.GetFriendList"
            android:label="@string/title_activity_get_friend_list" >
        </activity>
        <activity
            android:name="com.example.getimages.GridViewActivity"
            android:label="@string/title_activity_grid_view" >
        </activity>
        <activity
            android:name="com.example.getimages.FullImageActivity"
            android:label="@string/title_activity_full_image" >
        </activity>
        <activity
            android:name="com.example.getimages.ShakeStart"
            android:label="@string/title_activity_shake_start"
            >
            <intent-filter>
                <action android:name="intent.my.action" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
</activity>
        <activity
            android:name="com.example.getimages.SendActvity"
            android:label="@string/title_activity_send_actvity" >
        </activity>
    </application>

</manifest><!--
